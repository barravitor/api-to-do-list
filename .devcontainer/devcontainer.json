{
	"name": "to-do-list",
	"dockerComposeFile": [
		"docker-compose.yml"
	],
	"service": "to-do-list",
	"settings": {
	  "terminal.integrated.shell.linux": null
	},
	"extensions": [
		"ms-azuretools.vscode-docker",
		"editorconfig.editorconfig",
		"dbaeumer.vscode-eslint",
		"donjayamanne.githistory",
		"eamodio.gitlens",
		"ms-vscode.vscode-typescript-tslint-plugin",
		"waderyan.gitblame",
		"arjun.swagger-viewer",
		"redhat.vscode-yaml"
	],
	"workspaceMount": "source=${localWorkspaceFolder}/to-do-list,target=/home/to-do-list,type=bind,consistency=delegated",
	"workspaceFolder": "/home/to-do-list"
}
